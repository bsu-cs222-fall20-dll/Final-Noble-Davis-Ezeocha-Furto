<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.bsu.cs222.Controller"
            prefHeight="400.0" prefWidth="600.0">
    <Pane fx:id="individualGamePane" layoutX="250.0" layoutY="100.0" prefWidth="350" prefHeight="200">
        <fx:include fx:id="ticTacToe" source="TicTacToe.fxml" visible="false" />
    </Pane>
    <Label fx:id="gameName" alignment="CENTER" layoutY="24" layoutX="18" prefHeight="35" prefWidth="200">
        <font>
            <Font size="28.0" />
        </font>
    </Label>
    <Pane layoutY="24.0" layoutX="600.0" prefHeight="75" prefWidth="25" style="-fx-color: Green">
        <fx:include source="ProgressBar.fxml" fx:id="progressBarController"/>
    </Pane>
    <Label fx:id="gameNotificationLabel" alignment="CENTER" layoutX="324.0" layoutY="24.0" prefHeight="60.0" prefWidth="291.0" textAlignment="CENTER" wrapText="true">
        <font>
            <Font size="20.0" />
        </font>
    </Label>
    <Button fx:id="startDailyChallengeButton" layoutX="18.0" layoutY="104.0" contentDisplay="CENTER" mnemonicParsing="false" textAlignment="CENTER">
    </Button>
    <Button fx:id="instruction" layoutX="264.0" layoutY="24.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" text="help">
        <font>
            <Font size="28.0" />
        </font>
    </Button>
</AnchorPane>
