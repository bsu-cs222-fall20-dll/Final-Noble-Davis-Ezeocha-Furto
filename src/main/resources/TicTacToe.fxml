        <Button fx:id="cell6" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#CellSix" prefHeight="160.0" prefWidth="160.0" GridPane.rowIndex="2">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell7" layoutX="344.0" layoutY="177.0" mnemonicParsing="false" onAction="#CellSeven" prefHeight="160.0" prefWidth="160.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell8" layoutX="177.0" layoutY="344.0" mnemonicParsing="false" onAction="#CellEight" prefHeight="160.0" prefWidth="160.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
            <font>
