<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.bsu.cs222.TicTacToeController"
            prefHeight="400.0" prefWidth="600.0">
    <GridPane fx:id="board" gridLinesVisible="true" prefHeight="400.0" prefWidth="400.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Button fx:id="cell0" mnemonicParsing="false" onAction="#CellZero" prefHeight="160.0" prefWidth="160.0">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell1"  layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#CellOne" prefHeight="160.0" prefWidth="160.0" GridPane.columnIndex="1">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell2" layoutX="177.0" layoutY="10.0" mnemonicParsing="false" onAction="#CellTwo" prefHeight="160.0" prefWidth="160.0" GridPane.columnIndex="2">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell3" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#CellThree" prefHeight="160.0" prefWidth="160.0" GridPane.rowIndex="1">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell4" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#CellFour" prefHeight="160.0" prefWidth="160.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <font>
                <Font size="80.0"/>
            </font>
        </Button>
        <Button fx:id="cell5" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#CellFive" prefHeight="160.0" prefWidth="160.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <font>
